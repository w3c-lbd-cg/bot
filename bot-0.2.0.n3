bot:    a                              voaf:Vocabulary , owl:Ontology ;
        rdfs:seeAlso                   <https://doi.org/10.24928/JC3-2017/0153> , <https://www.researchgate.net/publication/320631574_Recent_changes_in_the_Building_Topology_Ontology> ;
        dce:Language                   "en" ;
        dce:description                "A simple ontology describing the topology of a building" ;
        dce:title                      "BOT" ;
        dcterms:contributor            "Maxime Lefrançois" , "Georg Ferdinand Schneider" ;
        dcterms:contributor            <http://maxime-lefrancois.info/me#> , <https://orcid.org/0000-0002-2033-859X> ;
        dcterms:creator                "Pieter Pauwels" , "Mads Holten Rasmussen" ;
        dcterms:creator                <https://orcid.org/0000-0001-8020-4609> , <https://orcid.org/0000-0002-6519-7057> ;
        dcterms:description            "The Building Topology Ontology (BOT) is a simple ontology defining the core concepts of a building.\nIt is a simple, easy to extend ontology for the construction industry to document and exchange building data on the web.\n\nVersion 0.2.0 of the ontology is documented in:\n\nMads Holten Rasmussen, Pieter Pauwels, Maxime Lefrançois, Georg Ferdinand Schneider, Christian Anker Hviid and Jan Karlshøj (2017) Recent changes in the Building Topology Ontology, 5th Linked Data in Architecture and Construction Workshop (LDAC2017), November 13-15, 2017, Dijon, France, https://www.researchgate.net/publication/320631574_Recent_changes_in_the_Building_Topology_Ontology\n\nThe initial version of the ontology was documented in:\n\nMads Holten Rasmussen, Pieter Pauwels, Christian Anker Hviid and Jan Karlshøj (2017) Proposing a Central AEC Ontology That Allows for Domain Specific Extensions, Lean and Computing in Construction Congress (LC3): Volume I – Proceedings of the Joint Conference on Computing in Construction (JC3), July 4-7, 2017, Heraklion, Greece, pp. 237-244 https://doi.org/10.24928/JC3-2017/0153"@en ;
        dcterms:issued                 "2017-10-27"^^xsd:date ;
        dcterms:license                <https://creativecommons.org/licenses/by/1.0/> ;
        dcterms:modified               "2017-10-27"^^xsd:date ;
        dcterms:title                  "BOT: Building Topology Ontology"@en ;
        vann:preferredNamespacePrefix  "bot" ;
        vann:preferredNamespaceUri     bot: ;
        owl:priorVersion               <https://w3id.org/bot-0.1.0> ;
        owl:versionIRI                 <https://w3id.org/bot-0.2.0> ;
        owl:versionInfo                "0.2.0" .

dcterms:description  a  owl:AnnotationProperty .

bot:Element  a        owl:Class ;
        rdfs:comment  "Constituent of a construction entity with a characteristic technical function, form or position [12006-2, 3.4.7]"@en , "Bestanddeel van een gebouwde entiteit met een karakteristieke technische functie, vorm of positie [12006-2, 3.4.7]"@nl , "Bestanddel af et bygværk med en karakteristisk funktion, form eller position [12006-2, 3.4.7]"@da , "Das Bauteil ist im Bauwesen ein einzelnes Teil, ein Element oder eine Komponente, aus denen ein Bauwerk zusammengesetzt wird [12006-2, 3.4.7]"@de ;
        rdfs:label    "Gebouwelement"@nl , "Bauteil (Bauwesen)"@de , "Building element"@en , "Elemento architettonico"@it , "Bygningsdel"@da .

bot:Storey  a            owl:Class ;
        rdfs:comment     "Die Gesamtheit aller Räume in einem Gebäude, die auf einer Zugangsebene liegen und horizontal verbunden sind"@de , "Een horizontaal gedeelte van een gebouw"@nl , "A level part of a building"@en , "Et plan i en bygning"@da ;
        rdfs:label       "Verdieping"@nl , "Piano di edificio"@it , "Storey"@en , "Etage"@da , "Geschoss (Architektur)"@de ;
        rdfs:subClassOf  bot:Zone .
