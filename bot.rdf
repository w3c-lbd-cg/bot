    <rdf:type rdf:resource="https://schema.org/Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2006/vcard/ns#Individual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocommons/voaf#Vocabulary">
