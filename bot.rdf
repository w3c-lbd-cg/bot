  <rdf:Description rdf:about="https://w3id.org/bot#containsElement">
    <rdfs:label xml:lang="zh-cn">包含构件</rdfs:label>
    <rdfs:label xml:lang="it">contiene elemento</rdfs:label>
    <rdfs:comment xml:lang="es">Relación a un elemento arquitectónico contenido en una zona.</rdfs:comment>
    <rdfs:comment xml:lang="de">Beziehung einer Zone zu einem Bauteil, dass es enthält.</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vztah k stavebnímu prvku obsaženém v zóně.</rdfs:comment>
    <rdfs:label xml:lang="fr">contient élément</rdfs:label>
    <rdfs:label xml:lang="es">contiene elemento</rdfs:label>
    <rdfs:comment xml:lang="tr">Bir bölgede bulunan bir bina parçasıyla ilişki.</rdfs:comment>
    <rdfs:label xml:lang="en">contains element</rdfs:label>
    <rdfs:label xml:lang="pt">comtém elemento</rdfs:label>
    <rdfs:label xml:lang="fi">sisältää elementin</rdfs:label>
    <rdfs:comment xml:lang="sk">Vzťah k stavebnému prvku, obsiahnutom v zóne.</rdfs:comment>
    <rdfs:comment xml:lang="da">Relation til en bygningsdel, som er indeholdt i en zone.</rdfs:comment>
    <rdfs:label xml:lang="sk">obsahuje element-prvok</rdfs:label>
    <rdfs:comment xml:lang="ca">Relació a un element arquitectònic contingut en una zona.</rdfs:comment>
    <rdfs:label xml:lang="fa">المان دربرگیرنده</rdfs:label>
    <rdfs:comment xml:lang="nl">Relatie tussen zone en een gebouwelement bevat in die zone.</rdfs:comment>
    <rdfs:label xml:lang="nl">bevat element</rdfs:label>
    <rdfs:comment xml:lang="fr">Relation à définir entre un élément de bâti et la zone le contenant</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="it">Relazione con elementi di costruzione contenuti in una zona.</rdfs:comment>
    <vs:term_status>stable</vs:term_status>
    <rdfs:comment xml:lang="fi">Suhde vyöhykkeen sisältämään rakennuselementtiin.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/bot#"/>
    <rdfs:label xml:lang="cs">obsahuje element-prvek</rdfs:label>
    <rdfs:label xml:lang="de">enthält Bauteil</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#hasElement"/>
    <owl:propertyChainAxiom rdf:nodeID="A3"/>
    <rdfs:label xml:lang="ca">conté un element</rdfs:label>
    <rdfs:comment xml:lang="fa">رابطه بین یک منطقه با المان ساختمانی که آنرا در بر گرفته است.</rdfs:comment>
    <rdfs:comment xml:lang="zh-cn">区域中包含的建筑构件的关系。</rdfs:comment>
    <rdfs:comment xml:lang="pt">Relação com um elemento da construção contido numa zona.</rdfs:comment>
    <rdfs:label xml:lang="sv">innehåller byggdel</rdfs:label>
    <rdfs:label xml:lang="tr">Eleman icerir</rdfs:label>
    <rdfs:comment xml:lang="sv">Relation till en byggdel som inryms i en zon.</rdfs:comment>
    <rdfs:label xml:lang="da">indeholder bygningsdel</rdfs:label>
    <rdfs:comment xml:lang="en">Relation to a building element contained in a zone.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2003/06/sw-vocab-status/ns#term_status">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/description">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
